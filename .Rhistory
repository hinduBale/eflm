test_AICs <- function(L = list(fm1, fm2)) {
for(fm in L) {
print(ll <- logLik(fm))
p <- attr(ll, "df")
A0 <- AIC(fm)
A1 <- -2*c(ll) + 2*p
aic.v <- fm$family$aic(
y  = fm$y, mu = fitted(fm),
wt = weights(fm), dev= deviance(fm)
)
stopifnot(
p == (p. <- length(coef(fm))) + hasDisp,
all.equal(-2*c(ll) + 2*hasDisp, aic.v) # <fam>$aic() = -2 * loglik + 2s
)
A2 <- aic.v + 2 * p.
stopifnot(
exprs = {
all.equal(A0, A1)
all.equal(A1, A2)
all.equal(A1, fm$aic)
}
)
}
}
